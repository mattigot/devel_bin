#sudo wpa_supplicant -T -s -i wlan0 -c  ~/bin/wpa_supplicant_p2p.conf -B -dd -Dnl80211
#sudo wpa_supplicant -i wlan0 -c  ~/bin/wpa_supplicant_p2p.conf -B -d -Dnl80211
